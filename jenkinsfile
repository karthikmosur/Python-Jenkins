pipeline {
  agent { label 'ec2' }

  triggers {
    githubPush()
  }

  stages {
    stage('Checkout Code') {
      steps {
        checkout scm
      }
    }

    stage('Run Python App') {
      steps {
        sh '''
          python3 --version
          python3 app.py
        '''
      }
    }
  }
}
